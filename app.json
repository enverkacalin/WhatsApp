{
  "expo": {
    "name": "whatsapp",
    "slug": "whatsapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/image/whatsapp-icon.png",
    "splash": {
      "image": "./src/assets/image/splash-whatsapp.png",
      "resiveMode":"contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
